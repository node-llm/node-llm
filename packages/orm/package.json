{
  "name": "@node-llm/orm",
  "version": "0.1.0",
  "description": "Database persistence layer for NodeLLM - Chat, Message, and ToolCall tracking",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "node-llm-orm": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./prisma": {
      "import": "./dist/adapters/prisma/index.js",
      "types": "./dist/adapters/prisma/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "llm",
    "orm",
    "prisma",
    "chat",
    "persistence",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@node-llm/core": "^1.0.0",
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "@node-llm/core": "file:../core",
    "@prisma/client": "^5.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {}
}
